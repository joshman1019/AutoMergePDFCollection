<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <Version>1.2.0</Version>
    <Authors>Joshua H. Santiago</Authors>
    <Company>Molecule Software</Company>
    <Product>AutoMergePDF</Product>
    <Description>A library that can simply combine two PDF documents into one, or combine a list of PDF documents into one</Description>
    <Copyright>Developer claims no copyright</Copyright>
    <PackageTags>PDF Merge Molecule Software Merger</PackageTags>
    <PackageReleaseNotes>Added the ability to merge a list of PDF documents, now not just limited to two</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="itext7" Version="7.1.13" />
  </ItemGroup>

</Project>
